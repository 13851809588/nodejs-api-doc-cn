{
    "output": "../_website/",
    "generator": "site",
    "plugins": [
        "-search",
        "-fontsettings",
        "-sharing",
        "regexplace"
    ],
    "pluginsConfig": {
        "regexplace": {
            "substitutes": [
                {
                    "pattern": ".md#",
                    "flags": "g",
                    "substitute": ".html#"
                }
            ]
        }
    }
}
